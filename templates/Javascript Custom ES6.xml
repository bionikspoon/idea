<templateSet group="Javascript Custom ES6">
  <template name="imp" value="import $MODULE$ from '$LOCATION$$PACKAGE$';" toReformat="false" toShortenFQNames="true">
    <variable name="LOCATION" expression="fileNameWithoutExtension()" defaultValue="&quot;&quot;" alwaysStopAt="true" />
    <variable name="PACKAGE" expression="fileNameWithoutExtension()" defaultValue="" alwaysStopAt="true" />
    <variable name="MODULE" expression="PACKAGE" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>